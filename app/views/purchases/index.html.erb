<h1>Purchasing Users</h1>
<%= link_to "Add", new_purchase_path %><br>
<div class="col-md-4"><strong></string>User</div></strong><br>
<% Purchase.all.each do |purchase| %>
    <div class="col-md-4"><%= purchase.email %></div>
    <div class="col-md-4"><%= link_to "Edit", edit_purchase_path(purchase) %></div>
    <div class="col-md-4"><%= button_to "Delete", purchase, method: :delete %></div><br>
<% end %>